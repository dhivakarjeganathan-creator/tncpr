# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Application Settings
DEFAULT_TIMEZONE=UTC
ENABLE_DEBUG=False
